<div id="showMyAccount" style="padding-top: 20px">
	<div class="form-group">
	  <label for="name"><%= i18n("label_name") %>:</label>
	  <input type="text" class="form-control" name="name" id="name" value="<%= speaker.name %>" disabled>
	</div>
	<div class="form-group">
	  <label for="email"><%= i18n("label_email") %>:</label>
	  <input type="text" class="form-control" name="email" id="email" value="<%= speaker.email %>" disabled>
	</div>
	<div class="form-group">
	  <label for="password"><%= i18n("label_password") %>:</label>
	  <input type="text" class="form-control" name="password" id="password" value="<%= speaker.password %>" disabled>
	</div>
</div>